        "type": 2,
        "title": "Gitea",
        "description": "Code hosting utility",
            "note": "<p>Application website : <a href=\"https:\/\/gitea.io\/\" target=\"_blank\" rel=\"noopener\">gitea.io<\/a>",
        "categories": [
                    "Utility"
        ],
        "platform": "linux",
        "logo": "https://raw.githubusercontent.com/kallioli/docker-apps-collection/master/img/gitea_logo.png",
        "repository": {
            "stackfile": "swarm-stack/gitea.yml",
            "url": "https://github.com/linitio/docker-apps-collection"
        },
        "env": [{
            "name": "SERVICE",
            "label": "Service name",
            "description": "example: gitea"
        },
        {
            "name": "PUID",
            "label": "Gitea user id",
            "description": "example : 1001"
        },
        {
            "name": "PGID",
            "label": "Gitea group id",
            "description": "example : 1001"
        },
        {
            "name": "URL_LOKI",
            "label": "Loki service URL",
            "description": "example : http://loki.domain.tld/loki/api/v1/push"
        },
        {
            "name": "FQDN",
            "label": "Fully qualified domain name",
            "description": "example : git.domain.tld"
        },
        {
            "name": "DATA_LOCATION",
            "label": "Location of service data",
            "description": "example : /apps/'service'"
        },
        {
            "name": "NETWORK_APP",
            "label": "Application network name",
            "description": "example : app-network"
        }]
    },
