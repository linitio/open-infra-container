{
	"version": "2",
	"templates": [{
        "type": 3,
        "title": "Filebrowser",
        "description": "A python based web application for monitoring, analytics and notifications for Plex Media Server.",
	    "note": "<p>Application website : <a href=\"https:\/\/tautulli.com\/\" target=\"_blank\" rel=\"noopener\">tautulli.com<\/a>",
        "categories": [
		    "utility"
        ],
        "platform": "linux",
        "logo": "https://raw.githubusercontent.com/linitio/open-infra-container/main/img/filebrowser_logo.png",
        "repository": {
            "stackfile": "docker/filebrowser.yml",
            "url": "https://github.com/linitio/open-infra-container"
        },
        "env": [{
            "name": "SERVICE",
            "label": "Service name",
            "description": "example: filebrowser"
        },
        {
            "name": "PUID",
            "label": "User plex id",
            "description": "1001"
        },
        {
            "name": "PGID",
            "label": "User group plex id",
            "description": "1001"
        },
        {
            "name": "TZ",
            "label": "Service time zone",
            "description": "(example : Europe/Paris)"
        },
        {
            "name": "DATA_LOCATION",
            "label": "Service data location",
            "description": "example : /app/service"
        },
        {
            "name": "JSON_CONFIG_FILE",
            "label": "Location of config.json file",
            "description": "example : /app/service/config.json"
        },
        {
            "name": "DB_FILE",
            "label": "Location of filebrowser.db file",
            "description": "example : /app/service/filebrowser.db"
        },
        {
            "name": "BRANDING_DIR",
            "label": "Folder for customization files",
            "description": "example : /app/service/customization"
        },
        {
            "name": "FQDN",
            "label": "Fully qualified domain name",
            "description": "example : tautulli.domain.tld"
        }]
    },
    {
        "type": 3,
        "title": "Plex",
        "description": "Plex organizes video, music and photos from personal media libraries and streams them to smart TVs, streaming boxes and mobile devices.",
	    "note": "<p>Application website : <a href=\"https:\/\/plex.tv\/\" target=\"_blank\" rel=\"noopener\">plex.tv<\/a>",
        "categories": [
		    "multimedia"
        ],
        "platform": "linux",
        "logo": "https://raw.githubusercontent.com/linitio/open-infra-container/main/img/plex_logo.png",
        "repository": {
            "stackfile": "docker/plex.yml",
            "url": "https://github.com/linitio/open-infra-container"
        },
        "env": [{
            "name": "SERVICE",
            "label": "Service name",
            "description": "example: plex"
        },
        {
            "name": "VERSION",
            "label": "Select version",
            "description": "example: latest",
            "default": "docker"
        },
        {
            "name": "PUID",
            "label": "User plex id",
            "description": "1001"
        },
        {
            "name": "PGID",
            "label": "User group plex id",
            "description": "1001"
        },
        {
            "name": "TZ",
            "label": "Service time zone",
            "description": "(example : Europe/Paris)"
        },
        {
            "name": "CONFIG_DATA_LOCATION",
            "label": "Service config data location",
            "description": "example /app/ service"
        },
        {
            "name": "DATA_LOCATION",
            "label": "Service data location",
            "description": "example /app/ service"
        },
        {
            "name": "TRANSCODE_DATA_LOCATION",
            "label": "Data transcode location",
            "description": "/app "
        }]
    },

